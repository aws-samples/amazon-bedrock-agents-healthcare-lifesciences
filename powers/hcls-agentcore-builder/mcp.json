{
  "mcpServers": {
    "agentcore-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.amazon-bedrock-agentcore-mcp-server@latest"
      ],
      "disabled": false,
      "autoApprove": [
        "search_agentcore_docs"
      ]
    },
    "strands-agents": {
      "command": "uvx",
      "args": [
        "strands-agents-mcp-server"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "INFO"
      },
      "autoApprove": [
        "search_docs",
        "fetch_doc"
      ]
    }
  }
}
